{
  "name": "Hello World!",
  "description": "My first packaged app.",
  "version": "1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
	  "storage",
	  "http://*.extempengine.com/",
	  "unlimitedStorage"
	],
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "img/calculator-16.png",
    "128": "img/calculator-128.png"
  }
}